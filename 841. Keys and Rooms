class Solution {
    private void dfs(int node, int[] vis, List<List<Integer>> rooms) {
        vis[node] = 1;
        for(int child: rooms.get(node)) {
            if(vis[child] == 0) {
                vis[child] = 1;
                dfs(child, vis, rooms);
            }
        }
    }
    public boolean canVisitAllRooms(List<List<Integer>> rooms) {
        int n = rooms.size();
        int[] vis = new int[n];
        dfs(0, vis, rooms);

        for(int i=0; i<n; i++) {
            if(vis[i] == 0) return false;
        }
        return true;
    }
}
