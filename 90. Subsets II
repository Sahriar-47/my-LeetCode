class Solution {
    void recur(int ind, int len, int[] nums, Set<List<Integer>>subs, 
    List<Integer> cur) {
        if(ind >= len) {
            subs.add(new ArrayList<>(cur));
            return;
        }

        cur.add(nums[ind]);
        recur(ind+1, len, nums, subs, cur);
        cur.remove(cur.size()-1);
        recur(ind+1, len, nums, subs, cur);
        return;
    }
    public List<List<Integer>> subsetsWithDup(int[] nums) {
        int len = nums.length;
        List<List<Integer>> ans = new ArrayList<>();
        Set<List<Integer>> subs = new HashSet<>();
        Arrays.sort(nums);
        recur(0, len, nums, subs, new ArrayList<>());

        for(List<Integer> li: subs) {
            ans.add(new ArrayList<>(li));
        }
        return ans;
    }
}
