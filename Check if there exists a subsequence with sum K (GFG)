// User function Template for Java

class Solution {
    private static boolean check(int n, int[] arr, int ind, int sum, int k) {
        if(n == ind) {
            if(sum == k)
            return true;
            
            return false;
        }
        
        sum += arr[ind];
        boolean pick = check(n, arr, ind+1, sum, k);
        
        sum -= arr[ind];
        boolean notPick = check(n, arr, ind+1, sum, k);
        
        return pick|notPick;
    }
    public static boolean checkSubsequenceSum(int N, int[] arr, int K) {
        // code here
        return check(N, arr, 0, 0, K);
    }
}
